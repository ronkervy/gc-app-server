{
  "name": "gc-app-server",
  "productName": "gc-app-server",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "set NODE_ENV=development && electron-forge start",
    "start-inspect": "electron-forge start --enable-logging --inspect-electron",
    "build": "react-scripts build",
    "build:package": "set NODE_ENV=production && react-scripts build && electron-forge package",
    "package": "set NODE_ENV=production && electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "serve": "set NODE_ENV=development && electron --trace-warnings ."
  },
  "keywords": [],
  "author": {
    "name": "kervy",
    "email": "kervy00@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "electronRebuildConfig": {},
      "packagerConfig": {
        "prune": true,
        "asar": {
          "unpack": "**/app/config/default.json",
          "unpackDir": "{**/src/pdfs,**/src/native_modules}"
        },
        "icon": "./src/app/logo.png"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "gc_app_server"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-auto-unpack-natives",
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-react": "^7.14.5",
    "@electron-forge/cli": "^6.0.0-beta.53",
    "@electron-forge/maker-deb": "^6.0.0-beta.53",
    "@electron-forge/maker-rpm": "^6.0.0-beta.53",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.53",
    "@electron-forge/maker-zip": "^6.0.0-beta.53",
    "@electron-forge/plugin-auto-unpack-natives": "^6.0.0-beta.57",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.53",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/fs-extra": "^9.0.12",
    "babel-loader": "8.1.0",
    "bindings": "^1.5.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^4.3.0",
    "electron": "^14.0.0-beta.13",
    "electron-devtools-installer": "^3.2.0",
    "electron-installer-common": "^0.10.3",
    "electron-wix-msi": "^3.2.0",
    "exe-icon-extractor": "^1.0.8",
    "file-uri-to-path": "^1.0.0",
    "mongodb-client-encryption": "^1.2.6",
    "nan": "^2.14.2",
    "node-loader": "^2.0.0",
    "string-replace-webpack-plugin": "^0.1.3",
    "style-loader": "^1.3.0",
    "tmp-promise": "^3.0.2",
    "transform-loader": "^0.2.4",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@date-io/moment": "^1.3.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@material-ui/pickers": "^3.3.10",
    "@reduxjs/toolkit": "^1.6.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "chart.js": "^3.5.0",
    "cmd-printer": "^1.0.0",
    "cors": "^2.8.5",
    "date-fns": "^2.22.1",
    "electron-node-config": "^1.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "file-saver": "^2.0.5",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "helmet": "^4.6.0",
    "http-errors": "^1.7.2",
    "ip": "^1.1.5",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "mongodb": "^3.6.9",
    "mongoose": "^5.13.0",
    "node-ipc": "^9.1.4",
    "pdfmake": "^0.1.71",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.0.4",
    "react-dom": "^17.0.2",
    "react-number-format": "^4.6.3",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "socket.io": "^4.1.2",
    "socket.io-client": "^4.1.2",
    "uuid": "^3.4.0",
    "web-vitals": "^2.0.1"
  }
}
